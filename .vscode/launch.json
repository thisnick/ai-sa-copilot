{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger: Module: test2.py",
      "type": "debugpy",
      "request": "launch",
      "module": "api.test2",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger: Module: api.agents",
      "type": "debugpy",
      "request": "launch",
      "module": "api.agents",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "fastapi",
      "args": [
        "dev",
        "api",
        "--port",
        "8399",
        "--reload"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
